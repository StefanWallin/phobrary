<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>Digest</th>
      <th>Filetype</th>
      <th>Original filepath</th>
      <th>Current filepath</th>
      <th>Modifydate</th>
      <th>Createdate</th>
      <th>Imagewidth</th>
      <th>Imageheight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.digest %></td>
        <td><%= photo.filetype %></td>
        <td><%= photo.original_filepath %></td>
        <td><%= photo.current_filepath %></td>
        <td><%= photo.modifydate %></td>
        <td><%= photo.createdate %></td>
        <td><%= photo.imagewidth %></td>
        <td><%= photo.imageheight %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
